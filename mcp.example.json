{
  "mcpServers": {
    "aviation-local": {
      "command": "node",
      "args": ["./dist/index.js"],
      "env": {
        "API_NINJA_KEY": "",
        "FAA_CLIENT_ID": "",
        "FAA_CLIENT_SECRET": ""
      }
    },
    "aviation": {
