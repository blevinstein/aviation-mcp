{
  "mcpServers": {
    "weather": {
      "command": "npx",
      "args": ["-y", "aviation-mcp-weather"]
    },
    "charts": {
      "command": "npx",
      "args": ["-y", "aviation-mcp-charts"]
    },
    "precipitation": {
      "command": "npx",
      "args": ["-y", "aviation-mcp-precipitation"],
      "env": {
        "FAA_CLIENT_ID": "",
        "FAA_CLIENT_SECRET": ""
      }
    },
    "airports": {
      "command": "npx",
      "args": ["-y", "aviation-mcp-airports"],
      "env": {
        "FAA_CLIENT_ID": "",
        "FAA_CLIENT_SECRET": ""
      }
    },
    "notam": {
      "command": "npx",
      "args": ["-y", "aviation-mcp-notam"],
      "env": {
        "FAA_CLIENT_ID": "",
        "FAA_CLIENT_SECRET": ""
      }
    },
    "aircraft": {
      "command": "npx",
      "args": ["-y", "aviation-mcp-aircraft"],
      "env": {
        "API_NINJA_KEY": ""
      }
    }
  }
} 