{
  "mcpServers": {
    "weather": {
      "command": "npx",
      "args": ["-y", "aviation-weather-mcp"]
    },
    "charts": {
      "command": "npx",
      "args": ["-y", "aviation-charts-mcp"]
    },
    "precipitation": {
      "command": "npx",
      "args": ["-y", "aviation-precipitation-mcp"],
      "env": {
        "FAA_CLIENT_ID": "",
        "FAA_CLIENT_SECRET": ""
      }
    },
    "airports": {
      "command": "npx",
      "args": ["-y", "aviation-airports-mcp"],
      "env": {
        "FAA_CLIENT_ID": "",
        "FAA_CLIENT_SECRET": ""
      }
    },
    "notam": {
      "command": "npx",
      "args": ["-y", "aviation-notam-mcp"],
      "env": {
        "FAA_CLIENT_ID": "",
        "FAA_CLIENT_SECRET": ""
      }
    },
    "aircraft": {
      "command": "npx",
      "args": ["-y", "aviation-aircraft-mcp"],
      "env": {
        "API_NINJA_KEY": ""
      }
    }
  }
} 